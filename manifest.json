{
    "manifest_version": 2,

    "name": "StyleSnoop",
    "description": "My first Chrome extention",
    "version": "1.0",
    "author": "Kasper Beljaars",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Stylesnoop"
    },
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["js/script.js"]
        }
    ]
}